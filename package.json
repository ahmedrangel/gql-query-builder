  "homepage": "https://github.com/atulmy/gql-query-builder",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.6.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^9.9.0",
    "jest": "29.7.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.4",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "jest": {
